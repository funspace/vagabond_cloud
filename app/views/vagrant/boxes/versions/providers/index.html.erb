<h1>Listing vagrant_box_providers</h1>

<table>
  <thead>
    <tr>
      <th>Seq</th>
      <th>Name</th>
      <th>Url</th>
      <th>Version</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vagrant_box_version_providers.each do |vagrant_box_version_provider| %>
      <tr>
        <td><%= vagrant_box_version_provider.id %></td>
        <td><%= vagrant_box_version_provider.name %></td>
        <td><%= vagrant_box_version_provider.url %></td>
        <td><%= vagrant_box_version_provider.version %></td>
        <td><%= link_to 'Show', vagrant_box_version_provider_path(@vagrant_box, @vagrant_box_version, vagrant_box_version_provider) %></td>
        <td><%= link_to 'Edit', edit_vagrant_box_version_provider_path(@vagrant_box, @vagrant_box_version, vagrant_box_version_provider) %></td>
        <td><%= link_to 'Destroy', vagrant_box_version_provider_path(@vagrant_box, @vagrant_box_version, vagrant_box_version_provider), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Provider', new_vagrant_box_version_provider_path %>
